    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "concurrently": "^7.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "react-test-renderer": "17.0.1",
    "tailwindcss": "^3.0.13",
    "typescript": "~4.3.5"
  },
  "private": true
